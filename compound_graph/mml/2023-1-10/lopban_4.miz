    let n be Nat;
    thus (z rExpSeq).(n+1)=1/((n+1) !)*z #N (n+1) by Def2
      .=1/((n+1)!)*((z GeoSeq).n * z) by LOPBAN_3:def 9
      .=1/(n! * (n+1))*(z #N n * z) by NEWTON:15
      .=((1*1)/((n!) * (n+1)))*(z* z #N n ) by Lm1
