  end;
  seq_id x is absolutely_summable by Def1;
  then
A17: |.seq_id x.| is summable by COMSEQ_3:def 9;
  seq_id x is absolutely_summable by Def1;
  then
A18: |.seq_id x.| is summable by COMSEQ_3:def 9;
