  st
  D1=(DES-DIV8(Op-XOR(DES-E(R),RKey))).1 &
  D2=(DES-DIV8(Op-XOR(DES-E(R),RKey))).2 &
  D3=(DES-DIV8(Op-XOR(DES-E(R),RKey))).3 &
