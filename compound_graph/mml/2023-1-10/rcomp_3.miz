A41:  x < b by A40,XXREAL_0:2;
A42:  lower_bound Y = a1 by A38,A39,JORDAN5A:19;
      then a < a1 by A1,A3,A35,A36,A38,A39,XXREAL_0:1,XXREAL_1:1,4;
