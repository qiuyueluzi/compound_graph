    set G = t(#)abs f + t(#)abs g;
A21:dom F = E /\ E by A11,A12,VALUED_1:def 4;
A22:dom G = E /\ E by A18,VALUED_1:def 1;
    R_EAL F is_integrable_on M by A20; then
