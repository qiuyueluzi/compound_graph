A95: for v1,v2,v0 holds ((v1"/\"(v0"\/"v2))"/\"(v0"\/"v2))=(v1"/\"(v2"\/"v0))
proof let v1,v2,v0;
((v0"/\"(v1"/\"(v0"\/"v2)))"\/"(v2"/\"(v1"/\"(v0"\/"v2))))=
((v1"/\"(v0"\/"v2))"/\"(v0"\/"v2)) by A89;
